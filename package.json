{
  "name": "gredor-frontend",
  "version": "1.2.0",
  "author": "Potatron Tech AB",
  "license": "AGPL-3.0-or-later WITH romic-exception",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run start",
    "start": "npm run start:development",
    "start:development": "vite --mode development",
    "start:production": "vite --mode production",
    "build": "run-s generate-api-types generate-sie-mappings generate-taxonomy-types lint type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-and-preview": "npm run build && npm run preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint",
    "generate-api-types": "npx openapi-typescript src/openapi/gredor-backend-v1.yaml -o ./src/openapi/gredor-backend-v1.d.ts",
    "generate-sie-mappings": "npx tsx tools/sieMappingsGeneratorTool.ts -o src/data/taxonomy/k2/2021-10-31/sieMappings.ts",
    "generate-taxonomy-types": "npx tsx tools/taxonomyItemIdGeneratorTool.ts -o ./src/data/taxonomy/k2/2021-10-31/taxonomyItemIds.d.ts",
    "cy:open": "npx -y dotenv-cli -e .env.cypress -- start-server-and-test build-and-preview http://localhost:4173 'cypress open'",
    "cy:run": "npx -y dotenv-cli -e .env.cypress -- start-server-and-test build-and-preview http://localhost:4173 'cypress run'"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^14.1.0",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "byte-base64": "^1.1.0",
    "decimal.js": "^10.6.0",
    "linkify-string": "^4.3.2",
    "linkifyjs": "^4.3.2",
    "openapi-fetch": "^0.15.0",
    "validator": "^13.15.23",
    "vue": "^3.5.25",
    "vue-component-type-helpers": "^3.1.5",
    "vue-onboarding-tour": "^0.0.9-beta.5",
    "xml-formatter": "^3.6.7"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.7",
    "@tsconfig/node22": "^22.0.5",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^24.10.1",
    "@types/validator": "^13.15.10",
    "@types/yargs": "^17.0.35",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/tsconfig": "^0.7.0",
    "cheerio": "^1.1.2",
    "cypress": "^15.8.1",
    "cypress-delete-downloads-folder": "^0.0.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-chai-friendly": "^1.1.0",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "^10.6.2",
    "fast-xml-parser": "^5.3.2",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "json-diff-ts": "^4.8.2",
    "minimist": "^1.2.8",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript": "^7.10.1",
    "prettier": "3.7.4",
    "sass-embedded": "^1.93.3",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vite-plugin-istanbul": "^7.2.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.1.5",
    "yargs": "^18.0.0"
  }
}
