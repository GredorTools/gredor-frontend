  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run start",
    "start": "npm run start:development",
    "start:development": "vite --mode development",
    "start:production": "vite --mode production",
    "build": "run-s generate-api-types generate-taxonomy-types type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "generate-api-types": "npx openapi-typescript src/openapi/gredor-backend-v1.yaml -o ./src/openapi/gredor-backend-v1.d.ts",
    "generate-taxonomy-types": "npx tsx tools/taxonomyItemIdGeneratorTool.ts -o ./src/data/taxonomy/k2/2021-10-31/taxonomyItemIds.d.ts"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^13.6.0",
    "bootstrap": "^5.3.7",
    "byte-base64": "^1.1.0",
    "decimal.js": "^10.6.0",
    "openapi-fetch": "^0.14.0",
    "validator": "^13.15.15",
    "vue": "^3.5.18",
    "vue-component-type-helpers": "^3.0.5",
    "vue-onboarding-tour": "^0.0.9-beta.3",
    "vue3-select-component": "^0.11.8",
    "xml-formatter": "^3.6.6"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^24.2.1",
    "@types/validator": "^13.15.2",
    "@types/xlsx": "^0.0.35",
    "@types/yargs": "^17.0.33",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "minimist": "^1.2.8",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript": "^7.8.0",
    "prettier": "3.6.2",
    "sass-embedded": "^1.90.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.5",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.5",
    "yargs": "^18.0.0"
  }
}
